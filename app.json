{
  "expo": {
    "name": "SpeedySlotz",
    "slug": "speedySlotzfinalapp",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon-new.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "speedyslotz",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.speedyzslotznitinapp.app",
      "buildNumber": "49",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.speedyzslotznitinapp.app",
      "versionCode": 52,
      "permissions": [
        "NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ]
    },
    "plugins": [
      "expo-secure-store",
      "expo-system-ui",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon-new.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8cdc32df-1dfe-4ba1-b002-d69366d596e4"
      }
    }
  }
}
