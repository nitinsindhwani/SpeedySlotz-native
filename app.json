{
  "expo": {
    "name": "SpeedySlotz",
    "slug": "speedySlotzfinalapp",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "speedyslotz",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.speedyzslotznitinapp.app",
      "buildNumber": "10",
      "developmentClient": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.speedyzslotznitinapp.app",
      "versionCode": 15,
      "developmentClient": true,
      "permissions": [
        "NOTIFICATIONS"
      ]
    },
    "plugins": [
      "expo-secure-store",
      "expo-system-ui",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8cdc32df-1dfe-4ba1-b002-d69366d596e4"
      }
    }
  }
}
